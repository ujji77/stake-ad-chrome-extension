{
    "manifest_version": 3,
    "name": "Stake Ad Extension",
    "version": "1.0",
    "description": "Processes images on webpage through canvas",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [{
      "resources": ["stakelogo.png", "raisingcaneslogo.png"],
      "matches": ["<all_urls>"]
    }]
  }